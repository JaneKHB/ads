/CANON/DEVLOG/proxy_access.log {
	copytruncate
	daily
	rotate 7
	compress
	maxage 7
	missingok
	dateext
	ifempty
	create 0644 root root
}

/CANON/DEVLOG/proxy_error.log {
	copytruncate
	daily
	rotate 7
	compress
	maxage 7
	missingok
	dateext
	ifempty
	create 0644 root root
}

